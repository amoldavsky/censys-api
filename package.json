    "hono": "^4.9.0",
    "langchain": "^0.3.30",
    "mongoose": "^8.0.0",
    "mustache": "^4.2.0",
    "pino": "^9.8.0",
    "zod": "^4.0.15"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "bun-types": "^1.2.19",
    "pino-pretty": "^13.1.1",
